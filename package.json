    "vitest": "4.0.0-beta.7",
    "@vitest/browser": "4.0.0-beta.7"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.8.0",
    "nuxt": "4.0.3",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.14.0"
