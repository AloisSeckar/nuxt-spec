{
  "name": "nuxt-spec",
  "version": "0.1.8",
  "description": "Test-pack layer for Nuxt Applications",
  "repository": "github:AloisSeckar/nuxt-spec",
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "scripts": {
    "analyze": "nuxt analyze",
    "eslint": "eslint .",
    "build": "pnpm test && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "prepublishOnly": "pnpm build",
    "preview": "nuxt preview",
    "start": "nuxt start",
    "test": "vitest run",
    "test-u": "vitest run -u",
    "test-i": "vitest"
  },
  "bin": {
    "setup": "./bin/setup.js"
  },
  "exports": {
    "./config": {
      "import": "./config/index.mjs",
      "default": "./config/index.mjs"
    }
  },
  "files": [
    "app",
    "bin",
    "config",
    "public"
  ],
  "dependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.19.2",
    "@vitest/browser": "4.0.0-beta.11",
    "@vue/test-utils": "2.4.6",
    "elrh-cosca": "0.3.1",
    "happy-dom": "18.0.1",
    "nuxt": "4.1.2",
    "playwright-core": "1.55.0",
    "typescript": "5.9.2",
    "vitest": "4.0.0-beta.11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.16.1"
}
