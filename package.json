{
  "name": "nuxt-spec",
  "version": "0.1.1",
  "description": "Test-pack layer for Nuxt Applications",
  "repository": "github:AloisSeckar/nuxt-spec",
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "scripts": {
    "analyze": "nuxt analyze",
    "eslint": "eslint .",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "nuxt start",
    "test": "vitest run",
    "test-u": "vitest run -u",
    "test-i": "vitest"
  },
  "bin": {
